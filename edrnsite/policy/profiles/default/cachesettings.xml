                    <element value="orig_query"/>
                </property>
                <property name="etagComponents">
                    <element value="member"/>
                    <element value="catalog_modified"/>
                    <element value="language"/>
                    <element value="gzip"/>
                    <element value="skin"/>
                    <element value="b_start"/>
